<!DOCTYPE html>
<html>
	<head>
		<meta charset="UTF-8">
		<title>
			{% block title %}Bienvenu sur le site de Sandrine Coupart!
			{% endblock %}
		</title>
		<link rel="stylesheet" href="{{ asset('assets/css/bootstrap_1.min.css') }}">
		<link rel="stylesheet" href="{{ asset('assets/css/_variables_1.scss') }}">
		<link rel="stylesheet" href="{{ asset('assets/css/bootstrap_1.css') }}">
		<link href="https://fonts.googleapis.com/css2?family=Libre+Baskerville:wght@400&display=swap" rel="stylesheet">
		<link href="https://fonts.googleapis.com/css2?family=Karla:wght@300&display=swap" rel="stylesheet">
		<link rel="stylesheet" href="/style.css">
	
		{# Run `composer require symfony/webpack-encore-bundle` to start using Symfony UX #}
		{% block stylesheets %}
			{{ encore_entry_link_tags('app') }}
		{% endblock %}

		{% block javascripts %}
			{{ encore_entry_script_tags('app') }}
			<script src="https://cdn.jsdelivr.net/npm/@popperjs/core@2.9.2/dist/umd/popper.min.js" integrity="sha384-IQsoLXl5PILFhosVNubq5LC7Qb9DXgDA9i+tQ8Zj3iwWAwPtgFTxbJ8NT4GN1R8p" crossorigin="anonymous"></script>
			<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/js/bootstrap.min.js" integrity="sha384-cVKIPhGWiC2Al4u+LWgxfKTRIcfu0JTxR+EQDz/bgldoEyl4H0zUF0QKbrJ0EcQF" crossorigin="anonymous"></script>
		{% endblock %}
	</head>
	<body style="display: flex; flex-direction: column; min-height: 100vh; width: 100%; background-color: rgb(130 215 233);">
		{% block header %}
		{% include "partials/_nav.html.twig" %}
		{% endblock %}
	
		{% block body %}{% endblock %}
		
		{% include "partials/_footer.html.twig" %}
	</body>
</html>
